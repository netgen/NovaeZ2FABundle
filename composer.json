{
    "name": "netgen/ibexa-2fa-bundle",
    "description": "Netgen ibexa 2FA Bundle is an ibexa bundle that provides two-factor authentication for your ezplatform project",
    "keywords": [
        "two-factor",
        "authentication",
        "ezpublish",
        "novactive",
        "ezplatform"
    ],
    "homepage": "https://github.com/netgen/NovaIbexa2FABundle",
    "type": "ibexa-bundle",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "https://www.netgen.io",
            "email": "info@netgen.io"
        },
        {
            "name": "Borna Matijanic",
            "homepage": "https://github.com/ludwig031",
            "email": "borna.m@netgen.io"
        }
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": "^8.1",
        "scheb/2fa-bundle": "^6.9",
        "scheb/2fa-google-authenticator": "^6.9",
        "scheb/2fa-totp": "^6.9",
        "scheb/2fa-trusted-device": "^6.9",
        "scheb/2fa-backup-code":  "^6.9",
        "scheb/2fa-email": "^6.9",
        "bacon/bacon-qr-code": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Netgen\\Bundle\\Ibexa2FABundle\\": "bundle"
        }
    },
    "extra": {
        "thanks": [
            {
                "name": "novactive/Ibexa2FABundle",
                "url": "https://github.com/Novactive/NovaIbexa2FABundle"
            },
            {
                "name": "ezsystems/ezplatform",
                "url": "https://github.com/ezsystems/ezplatform"
            }

        ]
    }
}
